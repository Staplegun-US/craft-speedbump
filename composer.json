{
    "name": "staplegun/speedbump",
    "description": "Alerts users when navigating to any un-whitelisted links.",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "speedbump"
    ],
    "support": {
        "docs": "https://staplegun.us",
        "issues": "https://staplegun.us"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "STAPLEGUN",
            "homepage": "https://staplegun.us"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "staplegun\\speedbump\\": "src/"
        }
    },
    "extra": {
        "name": "Speedbump",
        "handle": "speedbump",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://github.com/Staplegun-US/craft-speedbump/blob/master/CHANGELOG.md",
        "class": "staplegun\\speedbump\\Speedbump"
    }
}
